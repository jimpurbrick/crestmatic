{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "/",
    "properties": {
        "documents": {
            "id": "documents",
            "items": {
                "id": "0",
                "properties": {
                    "documentKey": {
                        "description": "Unique key to identify this document, independent of version",
                        "id": "documentKey",
                        "type": "string"
                    },
                    "documentLocation": {
                        "description": "The location of the document.",
                        "id": "documentLocation",
                        "type": "string"
                    },
                    "documentType": {
                        "description": "type of document URL",
                        "id": "documentType",
                        "type": "string"
                    },
                    "resetDate": {
                        "description": "Date when confirmations for the document were last manually reset",
                        "id": "resetDate",
                        "type": "string"
                    }
                },
                "required": [
                    "documentKey",
                    "documentLocation",
                    "documentType"
                ],
                "type": "object"
            },
            "type": "array"
        }
    },
    "required": [
        "documents"
    ],
    "type": "object"
}