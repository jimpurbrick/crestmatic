{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "/",
  "properties": {
    "documents": {
      "id": "documents",
      "items": {
        "id": "0",
        "properties": {
          "documentKey": {
            "description": "Unique key to identify this document, independent of version",
            "id": "documentKey",
            "type": "string"
          },
          "documentType": {
            "description": "type of document URL",
            "id": "documentType",
            "type": "string"
          },
          "href": {
            "description": "URL where the content of the document can be found",
            "id": "href",
            "type": "string"
          },
          "resetDate": {
            "description": "Date when confirmations for the document were last manually reset",
            "id": "resetDate",
            "type": "string"
          }
        },
        "required": [
          "href",
          "documentKey",
          "documentType"
        ],
        "type": "object"
      },
      "type": "array"
    }
  },
  "required": [
    "documents"
  ],
  "type": "object"
}